<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sarika.myapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddDetail"
            android:label="@string/title_activity_add_detail"></activity>
        <activity
            android:name=".RemoveDetail"
            android:label="@string/title_activity_remove_detail"></activity>
        <activity
            android:name=".ViewActivity"
            android:label="@string/title_activity_view"></activity>
        <activity
            android:name=".FileSaving"
            android:label="@string/title_activity_file_saving"></activity>
        <activity
            android:name=".Check"
            android:label="@string/title_activity_check"></activity>
        <activity
            android:name=".CreatePass"
            android:label="@string/title_activity_create_pass"></activity>
        <activity
            android:name=".CreatePassword"
            android:label="@string/title_activity_create_password"></activity>
        <activity
            android:name=".ModifyPassword"
            android:label="@string/title_activity_modify_password"></activity>
    </application>

</manifest>
